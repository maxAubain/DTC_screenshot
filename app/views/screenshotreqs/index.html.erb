<%# View for home page. %>

<hr>
<h4> <%= link_to 'Make A Screenshot Request', new_screenshotreq_path %> </h4>
<%# Link to new.html.erb for new screenshot request. %>

<hr>
<h3>Stored Requests</h3>
<% @screenshotreqs.each do |screenshotreq| %>
  <div>
    <p><%= screenshotreq.name %>. <%= link_to 'Show', screenshotreq_path(screenshotreq) %> &nbsp; <%= link_to 'Delete', screenshotreq_path(screenshotreq), method: :delete%>
  </div>
<% end %>
<%# Show a history of all screenshotreqs with options to see results and delete. %>